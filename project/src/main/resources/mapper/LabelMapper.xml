<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.collectplatform.project.dao.TagDao">
    <resultMap id="resourceInfo" type="com.collectplatform.project.vo.TagVo.ListOutVo">
        <result property="id" column="id"></result>
        <result property="parentId" column="parent_id"></result>
        <result property="name" column="name"></result>
        <result property="addTime" column="add_time"></result>
        <result property="updateTime" column="update_time"></result>
    </resultMap>

    <select id="listPage" resultMap="resourceInfo">
        SELECT
            id, parent_id, `name`,add_time,update_time
        FROM
            proj_label
        ${ew.customSqlSegment}
    </select>

    <select id="listAll" resultMap="resourceInfo">
        SELECT
            id, parent_id, `name`,add_time,update_time
        FROM
            proj_label
        ${ew.customSqlSegment}
    </select>

    <select id="labelInfoById" resultMap="resourceInfo">
        SELECT
            id, parent_id, `name`,add_time,update_time
        FROM
            proj_label
        ${ew.customSqlSegment}
    </select>

    <select id="labelInfoByParentId" resultMap="resourceInfo">
        SELECT
            id, parent_id, `name`,add_time,update_time
        FROM
            proj_label
        ${ew.customSqlSegment}
    </select>
</mapper>